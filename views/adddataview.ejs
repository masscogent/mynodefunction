<%- include('head.ejs',{pagetitle:title}) -%>

<div class="container mt-5">
  <h3>Add Data Into Database</h3>
  <form action="/mydata/add_database_data" enctype="multipart/form-data" method="post" class="mt-3">
    <div class="mb-3">
        <label for="" class="form-label">Image</label>
        <input type="file" 
          class="form-control" name="profile_image" id="" aria-describedby="helpId" placeholder="" >
       
      </div>
    <div class="mb-3">
      <label for="" class="form-label">Name</label>
      <input
        type="text"
        class="form-control"
        name="name"
        id=""
        aria-describedby="helpId"
        placeholder=""
        required
      />
    </div>
    <div class="mb-3">
      <label for="" class="form-label">Address</label>
      <input
        type="text"
        class="form-control"
        name="address"
        id=""
        aria-describedby="helpId"
        placeholder=""
        required
      />
    </div>
    <div class="mb-3">
      <label for="" class="form-label">Phone</label>
      <input
        type="number"
        class="form-control"
        name="phone"
        id=""
        aria-describedby="helpId"
        placeholder=""
        required
      />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>

<%- include('foot.ejs') -%>
